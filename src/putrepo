
r=repo
rm -f $r/*

cp lozza.js $r
cp loss.py $r
cp bench $r
cp datagen.js $r
cp datagen $r
cp expjs $r
cp f $r
cp hosts $r
cp impfens $r
cp lozza.rs $r
cp makefile $r
cp prof $r
cp scanpgn $r
cp sprt $r
cp text2bullet $r
cp putrepo $r
cp ../bullet/go $r
cp top $r

node lozza.js serialise q > $r/weights.js
node lozza.js n q > $r/net.txt

losspath=$(grep -oP "const NET_WEIGHTS_FILE = '\K.*/nets/[^/]+" lozza.js)
echo $losspath
cp $losspath/loss.png $r

weightspath=$(grep -oP "const NET_WEIGHTS_FILE = '\K[^']+" lozza.js)
echo $weightspath
cp $weightspath $r

ls -la $r
